
 <% if current_user %>
      <script>
        window.currentUser = { 
          "id": <%= current_user.id %>,
          "username": "<%= current_user.username %>",
          "email": "<%= current_user.email %>",
          "pictureUrl": "<%= url_for(current_user.profile_picture) %>",
          };
      </script>
    <% end %>



<main id="root"></main>